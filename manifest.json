{
    "manifest_version": 2,
    "name": "GithubEmoji",
    "version": "1.0",
    "description": "Provides you with different emojis for github.",
    "icons": {
        "128":"icon128.png",
        "48":"icon48.png",
        "16":"icon16.png"
    },
    "browser_action":{
        "default_icon":"icon16.png",
        "default_popup":"popup.html"
    }
}